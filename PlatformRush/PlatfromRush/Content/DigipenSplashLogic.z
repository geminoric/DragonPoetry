class DigipenSplashLogic : ZilchComponent
{
    function Initialize(init : CogInitializer)
    {
        Zero.Connect(this.Space, Events.LogicUpdate, this.OnLogicUpdate);
    }

    function OnLogicUpdate(event : UpdateEvent)
    {
        var start = Level.Find("LambaAlpaca");
        
        Console.WriteLine(event.RealTimePassed);
        
        if(event.RealTimePassed > 3)
        {
            this.Space.LoadLevel(start);
        }
    }
}
